<form action="/update" method="post">
  <input type="text" name="name" id="name" value="<%= data.name%>" /><br />
  <select name="batch" id="batch">
    <option disabled selected>Please Choose Batch</option>
    <option value="MERN" <%= data.batch === 'MERN' ? 'selected' : ''%>>MERN</option>
    <option value="MEAN" <%= data.batch === 'MEAN' ? 'selected' : ''%>>MEAN</option>
    <option value="MEVN" <%= data.batch === 'MEVN' ? 'selected' : ''%>>MEVN</option></select
  ><br />
    <select name="class">
    <option disabled selected>Please Choose Class</option>
    <% for(let i=1; i<=12; i++) {%>
    <option value="<%= i %>" <%= data.class === i ? 'selected' : ''%> ><%= i %></option>
    <% } %></select
  ><br />
  <input type="submit" name="Submit" />
</form>

<a href="/listing">List</a>
